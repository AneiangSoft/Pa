{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Scraper": {
    // 数据缓存配置：None / Memory / Redis
    // 默认缓存时间 1 小时（可改）
    "CacheProvider": "Memory",

    // Redis 缓存配置（CacheProvider=Redis 时启用）
    //"Redis": {
    //  "Configuration": "localhost:6379,password=xxx,defaultDatabase=3",
    //  "InstanceName": "Aneiang.Pa:"
    //},

    "Authorization": {
      "Enabled": true,
      "Scheme": "ApiKey",
      "ApiKeys": [
        "demo-api-key-12345",
        "demo-api-key-67890"
      ],
      "ApiKeyHeaderName": "X-API-Key",
      "ApiKeyQueryParameterName": "apiKey",
      "UnauthorizedMessage": "未授权访问"
    }
  }
}
